signal finished()

var duration
var startDuration
var started

func start(duration):
	startDuration = duration
	self.duration = duration
	started = true
	
func _process(delta):
	if !started:
		return
	value = duration / startDuration * 100
	duration -= delta
	if duration < 0:
		emit_signal(\"finished\")
		started = false"

