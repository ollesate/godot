script/source = "extends VBoxContainer

var duration
var startDuration

func _ready():
	# showText(\"hi\")
	pass

func start(title, duration):
	$Label.text = title
	$Progress.show()
	self.duration = duration * 1.0
	startDuration = duration * 1.0

func showText(title):
	$Label.text = title
	$Progress.hide()
	
func _process(delta):
	if $Progress.visible:
		$Progress.value = duration / startDuration * 100
		duration -= delta"

