script/source = "extends Node2D

var playerScene = preload(\"res://Player.tscn\")
var spawns = [
	Vector2(320, 320), 
	Vector2(320, 448), 
	Vector2(704, 448)
]
var playerSpawns = {}
var players = {} setget , getPlayers

func spawnPlayer(playerInfo):
	if !playerSpawns.has(playerInfo.id):
		var next = playerSpawns.size()
		playerSpawns[playerInfo.id] = spawns[next]
	var player = playerScene.instance()
	player.position = playerSpawns[playerInfo.id]
	add_child(player)
	return player

func getPlayers():
	var players = {}
	for child in get_children():
		players[child.id] = child
	return players

func removePlayer(playerInfo):
	playerSpawns.erase(playerInfo.id)
	remove_child(players[playerInfo.id])"

